<Window x:Class="Organic_Browser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Organic_Browser"
        xmlns:control="clr-namespace:Organic_Browser.Controls"
        xmlns:cefSharp="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf" 
        mc:Ignorable="d"
        Title="Organic Browser" 
        Height="450" Width="800" 
        WindowState="Maximized"
        MinHeight="450"
        MinWidth="800">
        
    <Window.Resources>
        <Style x:Key="TabItemStyle" TargetType="TabItem">
            <Setter Property="Width" Value="180" />
            <Setter Property="Height" Value="30" />
        </Style>
    </Window.Resources>

    <control:BrowserTabControl x:Name="browserTabControl"></control:BrowserTabControl>
</Window>
